{
  "name": "coding-challenge",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --fix \"./**/*.{js,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-config-kdnj": "^0.0.11",
    "prettier": "2.4.1",
    "react-icons": "^4.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.6.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj/react"
    ],
    "root": true
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  }
}
